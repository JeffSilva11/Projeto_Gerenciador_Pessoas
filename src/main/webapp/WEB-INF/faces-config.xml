<?xml version="1.0" encoding="UTF-8"?>
<faces-config
        xmlns="http://xmlns.jcp.org/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
        version="2.3">

    <application>
        <el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
    </application>

    <converter>
        <converter-id>entityConverter</converter-id>
        <converter-class>io.github.jeffsilva11.com.br.projeto_pessoa_salario.converter.EntityConverter</converter-class>
    </converter>
    
    <converter>
        <converter-id>entityConverter2</converter-id>
        <converter-class>io.github.jeffsilva11.com.br.projeto_pessoa_salario.converter.EntityConverter2</converter-class>
    </converter>
    
    <converter>
    	<converter-id>dataConverter</converter-id>
    	<converter-class>io.github.jeffsilva11.com.br.projeto_pessoa_salario.converter.DataConverter</converter-class>
    </converter>
    
    <converter>
    	<converter-id>cargoConverter</converter-id>
    	<converter-class>io.github.jeffsilva11.com.br.projeto_pessoa_salario.converter.CargoConverter</converter-class>
    </converter>
    
    <converter>
    	<converter-id>cargoConverter2</converter-id>
    	<converter-class>io.github.jeffsilva11.com.br.projeto_pessoa_salario.converter.CargoConverter2</converter-class>
    </converter>


</faces-config>